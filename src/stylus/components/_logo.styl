.logo
  color #fff
  font-family 'Nixie One', cursive
  font-size 40px
  line-height 40px
  display block
  margin 10px auto
  padding 20px
  fill #fff
  transition .3s all
  strong
    position relative
    top -10px
    margin 0 10px
.logo-icon
  width 50px
  height 50px
  fill #fff
  path
    fill #fff
