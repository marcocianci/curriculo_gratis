.help
  position absolute
  top 2%
  right 20%
  z-index 999
  .btn_help
    font-size 0
    background none
    border 0
    outline none
    cursor pointer
    transition .2s all
    &:hover
      transform scale(1.3)
    &:active
      transform scale(1)
    svg
      width 60px
      fill #fff !important
      path
      fill #fff !important
